{
  "name": "cdaringe-blog",
  "description": "cdaringe-blog",
  "version": "0.0.1",
  "author": "cdaringe",
  "dependencies": {
    "gatsby": "^2.18.17",
    "gatsby-plugin-manifest": "^2.2.34",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-remark-copy-linked-files": "^2.0.5",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0"
  },
  "keywords": [
    "blog",
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby clean && gatsby build",
    "clean": "rm -rf .build .cache",
    "start": "gatsby develop",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write '{src,test,scripts}/**/*.{js,jsx,ts,tsx,md,mdx}' './*.{json,js,ts,md,mdx}'",
    "lint": "prettier --check '{src,test,scripts}/**/*.{js,jsx,ts,tsx,md,mdx}' './*.{json,js,ts,md,mdx}' --fix"
  },
  "devDependencies": {
    "gatsby-paginate": "^1.0.16",
    "gatsby-plugin-sharp": "^2.0.5",
    "gatsby-remark-images": "^2.0.3",
    "gatsby-remark-prismjs": "^3.0.0",
    "gatsby-source-filesystem": "^2.0.1",
    "gatsby-transformer-remark": "^2.1.3",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^2.1.1",
    "prismjs": "^1.15.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arstwtfwfwftwftwtfpdstfrphrphrpghpg"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,test,scripts}/**/*.{js,jsx,ts,tsx}": [
      "yarn format",
      "yarn lint",
      "git add"
    ]
  }
}
