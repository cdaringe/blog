{
  "name": "cdaringe-blog",
  "description": "cdaringe-blog",
  "version": "0.0.1",
  "author": "cdaringe",
  "dependencies": {
    "@mdx-js/react": "^2.0.0",
    "@parcel/core": "2.8.3",
    "ajv": "^6.12.6",
    "gatsby": "^5.12.9",
    "gatsby-paginate": "^1.1.1",
    "gatsby-plugin-image": "^3.12.3",
    "gatsby-plugin-manifest": "^5.12.3",
    "gatsby-plugin-mdx": "^5.12.3",
    "gatsby-plugin-page-creator": "^5.12.3",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-sharp": "^5.12.3",
    "gatsby-remark-copy-linked-files": "^6.12.0",
    "gatsby-remark-images": "^7.12.3",
    "gatsby-remark-prismjs": "^7.12.0",
    "gatsby-source-filesystem": "^5.12.1",
    "gatsby-transformer-remark": "^6.12.3",
    "gatsby-transformer-sharp": "^5.12.3",
    "husky": "8.0.3",
    "lint-staged": "15.1.0",
    "prettier": "^3.1.0",
    "prismjs": "^1.29.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0"
  },
  "keywords": [
    "blog",
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "gatsby build",
    "bd": "DEBUG=1 gatsby build",
    "predevelop": "npm run clean",
    "develop": "gatsby clean && gatsby develop",
    "clean": "gatsby clean",
    "start": "gatsby develop",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write '{src,test,scripts}/**/*.{js,jsx,ts,tsx,md,mdx}' './*.{json,js,ts,md,mdx}'",
    "lint": "eslint . --ext js,md,mdx && prettier --check '{src,test,scripts}/**/*.{js,jsx,ts,tsx,md,mdx}' './*.{json,js,ts,md,mdx}' --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arstwtfwfwftwftwtfpdstfrphrphrpghpg"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,test,scripts}/**/*.{js,jsx,ts,tsx}": [
      "yarn format",
      "yarn lint",
      "git add"
    ]
  },
  "devDependencies": {
    "eslint": "^8.54.0",
    "eslint-plugin-mdx": "^2.2.0",
    "eslint-plugin-react": "^7.33.2"
  }
}
